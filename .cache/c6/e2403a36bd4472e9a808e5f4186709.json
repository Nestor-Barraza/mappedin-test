name":null,"original":{"line":37,"column":2},"generated":{"line":57,"column":19}},{"source":"src/utils.ts","name":null,"original":{"line":37,"column":2},"generated":{"line":57,"column":27}}],"sources":{"src/utils.ts":"export function getDirectionIcon(direction: string): string {\n    switch (direction.toLowerCase()) {\n      case 'left': return '←';\n      case 'right': return '→';\n      case 'straight': return '↑';\n      default: return '•';\n    }\n  }\n  \n  export function roundDistance(distance: number): number {\n    return Math.round(distance);\n  }\n  \n  export function calculateEstimatedTime(distance: number): number {\n   return Math.ceil(distance / 80);\n  }\n  \n  export function formatDistance(distance: number): string {\n    const roundedDistance = roundDistance(distance);\n    return `${roundedDistance.toFixed(1)} meters`;\n  }\n  \n  export function sanitizeHTML(str: string): string {\n    const temp = document.createElement('div');\n    temp.textContent = str;\n    return temp.innerHTML;\n  }\n  \n  export function debounce(func: Function, wait: number): (...args: any[]) => void {\n    let timeout: any;\n    return function(...args: any[]) {\n      clearTimeout(timeout);\n      timeout = setTimeout(() => func.apply(this, args), wait);\n    };\n  }\n  \n  export function logError(message: string, error?: any): void {\n    console.error(`Error: ${message}`, error);\n  }"},"lineCount":null}},"error":null,"hash":"0a09c84f99c7eb0d60a1c07c13057b1e","cacheData":{"env":{}}}